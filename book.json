{
    "root": "./docs/",
    "title": "メールサーバーを保守管理する初心者に捧げる本",
    "structure": {
        "readme": "intoroduction.md",
        "summary": "_SUMMARY.md"
    },
    "styles": {
        "website": "styles/custom.css"
    },
    "plugins": [
        "navigator",
        "hide-published-with",
        "anchors",
        "hints",
        "-@honkit/honkit-plugin-ga"
    ],
    "pluginsConfig": {
        "ga": {
            "trackingID": "G-YYXXXXXX",
            "anonymizeIP": true
        }
    }
}